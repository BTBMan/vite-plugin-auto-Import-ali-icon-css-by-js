    "@ant-design/icons-vue",
    "iconfont",
    "css"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "author": "BTB",
  "license": "MIT",
  "devDependencies": {
    "@types/estree": "^0.0.51",
    "@types/micromatch": "^4.0.2",
    "bumpp": "^7.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.2",
    "vite": "^2.9.0",
    "vitest": "^0.8.1"
  },
  "dependencies": {
    "acorn": "^8.7.0",
    "micromatch": "^4.0.5"
  }
}
